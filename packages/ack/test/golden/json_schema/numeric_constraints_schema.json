{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "basicInt": {
      "type": "integer"
    },
    "rangeInt": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100
    },
    "positiveInt": {
      "type": "integer",
      "minimum": 1
    },
    "negativeInt": {
      "type": "integer",
      "maximum": -1
    },
    "multipleInt": {
      "type": "integer",
      "multipleOf": 5
    },
    "nullableInt": {
      "type": [
        "integer",
        "null"
      ]
    },
    "basicDouble": {
      "type": "number"
    },
    "rangeDouble": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 100.0
    },
    "multipleDouble": {
      "type": "number",
      "multipleOf": 0.5
    },
    "nullableDouble": {
      "type": [
        "number",
        "null"
      ]
    }
  },
  "additionalProperties": false
}